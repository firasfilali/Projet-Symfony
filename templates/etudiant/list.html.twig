    {{ etudiant | upper }}
    </li>
    {% else %}
    <strong>la liste des etudiants est vide</strong>
{% endfor %}

</ul>
<h1>Listes des modules</h1>
<table border="1">
    <tr>
        <td>idModule</td>
        <td>Nom</td>
        <td>Enseignant</td>
        <td>Nbre Heures</td>
        <td>Date</td>
    </tr>
    {% for m in ListModules %}
         <tr>
                <td>{{ m.id }}e</td>
                <td>{{ m.nom }}</td>
                <td>{{ m.enseignant }}</td>
                <td>{{ m.nbrheures }}</td>
                <td>{{ m.date }}</td>
             <td> <a href="{{ path('Affectation') }}">Affecter</a></td>
        </tr>
    {% endfor %}
</table>